/div>\n              </div>\n              <div class=\"field\">\n                <label class=\"label\">Lieu</label>\n                <div class=\"control\">\n                  <input class=\"input\" v-model=\"conversation.place\" />\n                </div>\n              </div>\n               <div class=\"field\">\n                <label class=\"label\">Date</label>\n                <div class=\"control\">\n                  <input class=\"input\" v-model=\"conversation.date\" />\n                </div>\n              </div>\n               <div class=\"field\">\n                <label class=\"label\">Heure</label>\n                <div class=\"control\">\n                  <input class=\"input\" v-model=\"conversation.heure\" />\n                </div>\n              </div>\n              <div class=\"buttons\">\n                <button class=\"button is-info is-outlined\">Poster</button>\n                <router-link class=\"button is-outlined\" to=\"/\"\n                  >Annuler</router-link\n                >\n              </div>\n            </form>\n          </div>\n        </div>\n      </div>\n    </div>\n  </section>\n</template>\n<script>\nexport default {\n  components: {},\n  data() {\n    return {\n      conversation: {\n        label: \"\",\n        topic: \"\",\n        place:\"\",\n        date:\"\",\n        heure:\"\",\n      },\n    };\n  },\n  methods: {\n    createConversation() {\n      this.$api\n        .post(\"channels\", this.conversation)\n        .then((response) => {\n          this.$router.push({\n            name: \"Conversation\",\n            params: { id: response.data.id },\n          });\n        })\n        .catch((error) => {\n          alert(error.response.data.message);\n        });\n    },\n  },\n};\n</script>\n<style scoped></style>\n"]}]}